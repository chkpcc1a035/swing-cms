{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ],
  "routes": [
    { "src": "/login", "dest": "/" },
    { "src": "/inventory", "dest": "/" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
